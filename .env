# Minimal config for testing
NODE_ENV=development
PORT=3000

# Database - using a test URL that will fail connection but allow Prisma client to load
POSTGRES_URL=postgresql://test:test@localhost:5432/echotune_test

# Disable optional features for testing
ENABLE_TRACING=false
ENABLE_AGENTOPS=false
DISABLE_REALTIME=true

# Spotify credentials - not required for startup test
SPOTIFY_CLIENT_ID=test
SPOTIFY_CLIENT_SECRET=test

# MongoDB - optional, but adding to prevent errors
MONGODB_URI=mongodb://localhost:27017/echotune_test
