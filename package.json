{
  "name": "spotify-echo",
  "version": "1.0.0",
  "description": "EchoTune AI - Sophisticated music recommendation system with AI-powered discovery",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "start:ci": "cross-env NODE_ENV=production PORT=${PORT:-3000} node server.js",
    "dev": "nodemon server.js",
    "profile": "vite",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=tests/integration --runInBand",
    "test:integration:live": "cross-env LIVE_VALIDATION=true USE_REAL_APIS=true node enhanced-integration-test.js",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:e2e": "playwright test",
    "test:e2e:live": "cross-env LIVE_VALIDATION=true USE_REAL_APIS=true playwright test",
    "test:e2e:smoke": "playwright test tests/e2e/smoke.spec.js --project=desktop-chromium",
    "test:comprehensive": "node scripts/run-comprehensive-tests.js",
    "test:installation": "node scripts/validate-installation.js",
    "test:mcp-servers": "node scripts/validate-mcp-servers.js",
    "test:api": "node scripts/run-api-validation.js",
    "test:api:discover": "node scripts/discover-api-endpoints.js",
    "test:api:comprehensive": "node scripts/comprehensive-api-tests.js",
    "test:api:report": "node scripts/generate-api-report.js",
    "test:api:full": "npm run test:api:discover && npm run test:api:comprehensive && npm run test:api:report",
    "validate:all": "npm run test:comprehensive",
    "validate:live": "cross-env LIVE_VALIDATION=true USE_REAL_APIS=true node comprehensive-validation-suite.js && node validate-integrated-system.js",
    "validate:env": "node scripts/validate-env.js",
    "auth:url": "node scripts/auth-helpers.js url",
    "auth:exchange": "node scripts/auth-helpers.js exchange",
    "auth:test-credentials": "node scripts/auth-helpers.js test-credentials",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "quality:check": "npm run lint && npm run security:scan",
    "quality:fix": "npm run lint:fix && npm run security:fix",
    "security:scan": "node scripts/security-scan.js",
    "security:scan:ci": "node scripts/security-scan.js --fail-on-high --fail-on-critical",
    "security:fix": "npm audit fix",
    "playwright:install": "playwright install --with-deps",
    "wait:health": "wait-on http://localhost:${PORT:-3000}/healthz",
    "e2e:with:server": "start-server-and-test \"npm run start:ci\" http://localhost:${PORT:-3000}/healthz \"npm run test:e2e\"",
    "qa:full": "node scripts/comprehensive-qa-automation.js",
    "qa:docker": "node scripts/docker-qa-automation.js",
    "qa:npm": "npm install && npm run test:comprehensive",
    "qa:all": "node scripts/master-qa-orchestrator.js",
    "qa:master": "node scripts/master-qa-orchestrator.js",
    "qa:validate": "node scripts/validate-qa-setup.js",
    "ci:status": "node scripts/generate-ci-status-report.js",
    "build": "node -e \"console.log('Noop build (server-only)')\"",
    "mcp:enhanced-validation": "node scripts/enhanced-mcp-validation-pipeline.js",
    "mcp:test:comprehensive": "node mcp-comprehensive-server-test.js",
    "mcp:test:live-servers": "node tests/integration/mcp-live-servers.test.js",
    "mcp:test:automation": "node tests/integration/mcp-testing-automation.test.js",
    "mcp:health:enhanced": "node src/mcp/EnhancedMCPValidatorV2.js",
    "ai:test": "node scripts/test-ai-providers.js all",
    "ai:test:providers": "node scripts/test-ai-providers.js providers",
    "ai:test:health": "node scripts/test-ai-providers.js health",
    "ai:health": "node -e \"const AgentRouter = require('./src/ai/agent/router'); const router = new AgentRouter(); router.healthCheck().then(console.log).catch(console.error)\"",
    "perplexity:research": "node -e \"const P = require('./src/chat/llm-providers/perplexity-provider'); const p = new P({apiKey: process.env.PERPLEXITY_API_KEY}); p.initialize().then(() => p.research(process.argv[1])).then(console.log).catch(console.error)\" -- ",
    "analysis:research": "node scripts/continuous-analysis-system.js single",
    "analysis:report": "echo 'Check automation-outputs/reports/ for latest analysis report'",
    "continuous-analysis:single": "node scripts/continuous-analysis-system.js single",
    "continuous-analysis:continuous": "node scripts/continuous-analysis-system.js continuous",
    "grok4:analyze": "node -e \"const G = require('./src/chat/llm-providers/grok4-provider'); const g = new G({apiKey: process.env.XAI_API_KEY}); g.initialize().then(() => g.analyzeRepository('sample code', 'comprehensive')).then(console.log).catch(console.error)\"",
    "ai:deploy:all-providers": "node scripts/deploy-ai-providers.js",
    "ai:deploy:all-providers:dry-run": "node scripts/deploy-ai-providers.js --dry-run",
    "ai:test:routing": "node scripts/test-ai-providers.js routing",
    "ai:test:claude-opus-comprehensive": "node scripts/test-claude-opus-4.1.js",
    "ai:eval": "node scripts/ai/run_eval.js run",
    "ai:eval:list": "node scripts/ai/run_eval.js list",
    "ai:eval:baseline": "node scripts/ai/run_eval.js run --model text-bison@latest --suite baseline_recommendations",
    "ai:analytics": "node -e \"const AgentRouter = require('./src/ai/agent/router'); const router = new AgentRouter(); console.log(JSON.stringify(router.getAnalytics(), null, 2))\"",
    "ai:metrics": "curl -s http://localhost:3000/metrics | grep echotune_ai || echo 'Metrics endpoint not available'",
    "validate:structure:health": "node scripts/validate-structure.js --health",
    "apply:indexes:analyze": "node scripts/apply-indexes.js --analyze",
    "build:cooccurrence": "node scripts/build-cooccurrence.js",
    "check:redis": "node scripts/check-redis.js",
    "e2e:chat-rec": "node scripts/run-e2e-chat-rec.js",
    "e2e:full": "npm run e2e:chat-rec",
    "test:realdata": "npm run test -- tests/recommendation/integration/realData.test.js",
    "mcp:deploy:browserbase": "cd mcp-servers/browserbase && npm run deploy",
    "mcp:logs": "cd mcp-servers && npm run logs",
    "comet:test": "node scripts/comet-browser-automation.js",
    "comet:test:headless": "cross-env COMET_HEADLESS=true node scripts/comet-browser-automation.js",
    "comet:test:record": "cross-env COMET_RECORDING=true node scripts/comet-browser-automation.js",
    "comet:test:performance": "cross-env PERFORMANCE_MONITORING=true node scripts/comet-browser-automation.js",
    "comet:test:accessibility": "cross-env A11Y_TESTING=true node scripts/comet-browser-automation.js",
    "comet:test:full": "npm run comet:test && npm run comet:test:performance && npm run comet:test:accessibility",
    "comet:cleanup": "rm -rf test-screenshots/* test-recordings/* test-reports/comet-*",
    "automation:full": "npm run mcp:health:enhanced && npm run comet:test:full && npm run validate:live",
    "automation:quick": "npm run comet:test:headless && npm run test:e2e:smoke",
    "yolo:enable": "echo 'YOLO mode enhanced - check .cursor/rules/yolo.md for configuration'",
    "yolo:test": "cross-env YOLO_MODE=true npm run automation:quick"
  },
  "keywords": [
    "music",
    "spotify",
    "ai",
    "recommendations",
    "analytics",
    "machine-learning",
    "conversational-ai",
    "browser-automation",
    "comet",
    "mcp-integration"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@google/generative-ai": "^0.2.1",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/resources": "^1.26.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/sdk-trace-base": "^1.26.0",
    "@opentelemetry/semantic-conventions": "^1.26.0",
    "@prisma/client": "^6.19.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.6.0",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.0",
    "express-session": "^1.17.3",
    "express-slow-down": "^3.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.6.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "openai": "^4.20.0",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "prisma": "^6.19.0",
    "prom-client": "^15.1.3",
    "prop-types": "^15.8.1",
    "react-router-dom": "^7.9.6",
    "recharts": "^3.4.1",
    "redis": "^4.6.0",
    "response-time": "^2.3.4",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "web-vitals": "^5.1.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.22",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.0.1",
    "pino-pretty": "^11.0.0",
    "react": "^19.2.0",
    "react-devtools-profiler": "^0.0.0",
    "react-dom": "^19.2.0",
    "sqlite3": "^5.1.7",
    "start-server-and-test": "^2.0.3",
    "terser": "^5.44.1",
    "vite": "^7.1.11",
    "vite-plugin-compression": "^0.5.1",
    "wait-on": "^7.2.0"
  }
}
